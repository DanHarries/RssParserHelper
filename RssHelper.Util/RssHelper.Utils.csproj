<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>RSS parser, RSS helper</PackageTags>
    <Authors>Dan H</Authors>
    <Product>RssParserHelper.NetCore</Product>
    <PackageId>Rss_Parser_Helper</PackageId>
    <Description>A simple library that will parse either a url or list of urls and return you an object of type Feed.</Description>
    <PackageIcon>rss_helper_library_icon.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Toolkit.Parsers" Version="6.1.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Desktop\rss_helper_library_icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
